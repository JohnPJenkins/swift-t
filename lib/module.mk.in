
DIR := lib

TCL_SRC := $(DIR)/helpers.tcl             \
           $(DIR)/turbine.tcl             \
           $(DIR)/engine.tcl              \
           $(DIR)/data.tcl

$(PACKAGE): lib/make-package.tcl $(TCL_TURBINE_SO) $(TCL_ADLB_SO) \
            $(TCL_SRC)
	$(Q) "  TCL		$(@)"
	$(E) tclsh $(<) > lib/pkgIndex.tcl
